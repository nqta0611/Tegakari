package tegakari;

import java.io.Serializable;

/**
 * This is the Enum that <code>Notepad</code> uses to mark off entries for the
 * robot to use.
 * 
 * @author Josh CHoi
 */
public enum NoteEntry implements Serializable
{
    /**
     * Notepad has not checked this entry off.
     */
    BLANK,
    /**
     * Notepad user knows that the entry was found.
     */
    HAS,
    /**
     * Notepad user knows the user won't have that entry.
     */
    HASNOT,
    /**
     * Notepad user has shown that entry to the another player.
     */
    SHOWN;
}
