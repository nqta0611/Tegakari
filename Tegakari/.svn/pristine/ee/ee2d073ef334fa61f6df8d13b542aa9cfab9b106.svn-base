package guiConsoleController;

import tegakari.Solution;

/**
 * The console for NoOneDisprove
 * @author DeionLaw
 */
public class NoOneDisproveConsole implements I_NoOneDisprove
{
    private Solution guess;
    
    /**
     * Constructor for NoOneDisproveConsole
     * @param aGuess the solution
     */
    public NoOneDisproveConsole(Solution aGuess)
    {
        this.guess = aGuess;
    }
    
    /**
     * Starts the console
     */
    public void startDialogue()
    {
        System.out.println("Suggestion Result: "
                    + "No one can disprove your suggestion.");
        System.out.println("Your guess was: " + guess.toString());
    }
    /**
     * disposes of the view
     */
    public void dispose()
    {
    }
}
